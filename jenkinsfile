pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'Hello build'
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello deploy'
            }
        }
        stage('testing') {
            steps {
                echo 'Hello testing the changes'
            }
        }
    }
}


